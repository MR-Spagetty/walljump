{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${mod_version}",
	"name": "${mod_name}",
	"description": "${mod_description}",
	"authors": [
		"${mod_author}"
	],
	"contributors": [
		"genandnic",
		"fzzyhmstrs"
	],
	"contact": {
		"homepage": "${mod_url}"
	},
	"license": "${mod_license}",
	"icon": "logo.png",
	"environment": "${side}",
	"entrypoints": {
		"main": [
			"com.jahirtrap.walljump.WallJumpMod"
		],
		"client": [
			"com.jahirtrap.walljump.WallJumpClient"
		]
	},
	"mixins": [
		"${mod_id}.mixins.json"
	],
	"depends": {
		"fabricloader": "${loader_version_range}",
		"minecraft": "${minecraft_version_range}",
		"java": ">=17",
		"fabric": "*"
	},
	"accessWidener" : "${mod_id}.accesswidener",
	"jars": [
		{"file": "META-INF/jars/configlibtxf-${configlibtxf_version}-fabric.jar"},
		{"file": "META-INF/jars/keybindfixtxf-${keybindfixtxf_version}.jar"}
	]
}
